<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.example.summerpracticesecondyear.entities.Ticket" table="tickets">
        <id name="id" column="id" type="int">
            <generator class="identity"/>
        </id>
        <property name="userId" column="user_id" type="int"/>
        <property name="sessionId" column="session_id" type="int"/>
        <property name="seat" column="seat" type="int"/>
    </class>
</hibernate-mapping>