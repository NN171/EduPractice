<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.example.summerpracticesecondyear.entities.Session" table="sessions">
        <id name="id" column="id" type="int">
            <generator class="identity"/>
        </id>
        <property name="movieId" column="movie_id" type="int"/>
        <property name="roomId" column="room_id" type="int"/>
        <property name="sessionDate" column="session_date" type="date"/>
        <property name="startTime" column="start_time" type="time"/>
        <property name="endTime" column="end_time" type="time"/>
    </class>
</hibernate-mapping>